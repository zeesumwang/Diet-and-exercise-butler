/* pages/foodhistory/foodhistory.wxss */
.history{
  background-color: white;
  padding-top: 50rpx;
  padding-bottom: 20rpx;
  display: flex;
  padding-left: 20rpx;
}
.date-btn{
  width: 220rpx;
  height:60rpx; 
  font-size: 28rpx;
  background-color: #eeeeee;
  padding-left: 10rpx;
  padding-right: 0rpx;
  margin-right: 0rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 100rpx;
}
.date-btn text{
  width: 170rpx;
}
.history-text{
  font-size: 28rpx;
  color:#999999;
  display: flex;
  align-items: center;
  margin-left: 15rpx;
  margin-right: 15rpx;
}
.chaxun{
  width: 100rpx;
  height:60rpx;
  font-size: 28rpx;
  color:white;
  background-color: #00bcd4;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 70rpx;
  margin-right: 0rpx;
  border-radius: 100rpx;
}
.canvas {
  width: 100%;
  height: 200px;
}
.historyTitle{
  display: flex;
  justify-content: space-between;
  padding-left: 30rpx;
  padding-right: 30rpx;
  font-size: 28rpx;
}
.historyData
{
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #8c8c8c;
}
